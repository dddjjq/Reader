<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TriangleIndicator">
        <attr name="text_color_not_select" format="color"/>
        <attr name="text_color_selected" format="color"/>
        <attr name="indicator_height" format="integer"/>
        <attr name="tab_item_count" format="integer"/>
        <attr name="text_size" format="dimension"/>
    </declare-styleable>
    <style name="GenderAnim">
        <item name="android:windowEnterAnimation">@anim/gender_anim_enter</item>
        <item name="android:windowExitAnimation">@anim/gender_anim_exit</item>
    </style>
    <declare-styleable name="RankLayout">
        <attr name="icon" format="reference"/>
        <attr name="name" format="string"/>
    </declare-styleable>
    <style name="CommunityPop">
        <item name="android:windowEnterAnimation">@anim/pop_anim_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_anim_exit</item>
    </style>
    <declare-styleable name="BookRedButton">
        <attr name="btnIcon" format="reference"/>
        <attr name="btnText" format="string"/>
        <attr name="backColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="BookReadEntityLayout">
        <attr name="entityTitle" format="string"/>
        <attr name="entityContent" format="string"/>
    </declare-styleable>
</resources>